{
    "name": "meowerapp",
    "env": {
        "MONGO_URI": "@meowerr-db-1"
    },
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}